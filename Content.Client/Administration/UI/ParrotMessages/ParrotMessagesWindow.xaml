<ui:FancyWindow xmlns="https://spacestation14.io"
                xmlns:parrotmessages="clr-namespace:Content.Client.Administration.UI.ParrotMessages"
                xmlns:ui="clr-namespace:Content.Client.UserInterface.Controls"
                VerticalExpand="True" HorizontalExpand="True"
                Title="{Loc parrot-messages-title}"
                SetSize="800 400">
    <PanelContainer StyleClasses="BackgroundDark">
        <TabContainer Name="MessageTabContainer" Access="Public">
            <parrotmessages:ParrotMessageList Name="ActiveMessages" Access="Public"></parrotmessages:ParrotMessageList>
            <parrotmessages:ParrotMessageList Name="BlockedMessages" Access="Public" ShowBlocked="True"></parrotmessages:ParrotMessageList>
        </TabContainer>
    </PanelContainer>
</ui:FancyWindow>
